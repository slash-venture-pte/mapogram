{
  "private": true,
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --fix --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "localstack": "docker compose -f docker-compose.local.yml up",
    "localstack:build": "docker compose -f docker-compose.local.yml build",
    "dev": "nodemon",
    "build": "yarn build:client && yarn build:server && yarn build:server:copies",
    "build:client": "node --unhandled-rejections=strict scripts/build.js",
    "build:server": "tsc --project tsconfig.server.json",
    "build:server:copies": "copyfiles -u 0 src/server/services/powershell/*.ps1 dist && copyfiles -u 1 src/server/public/** dist",
    "start": "cross-env NODE_ENV=production node dist/src/server/server.js",
    "type-check": "tsc",
    "prisma:generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "prisma db seed"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.1",
    "@mui/material": "^5.6.1",
    "@mui/styles": "^5.6.2",
    "@prisma/client": "^3.12.0",
    "@types/uuid": "^8.3.4",
    "autosuggest-highlight": "^3.2.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "fastq": "^1.13.0",
    "joi": "^17.6.0",
    "jszip": "^3.10.0",
    "loglevel": "^1.8.0",
    "next": "latest",
    "node-geocoder": "^4.0.0",
    "node-pg-migrate": "^6.2.1",
    "node-schedule": "^2.1.0",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-recaptcha-v3": "^1.9.7",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.17.0",
    "sequelize-typescript": "^2.1.3",
    "sharp": "^0.30.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^12.20.47",
    "@types/node-schedule": "^2.1.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/sharp": "^0.30.2",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.4",
    "copyfiles": "^2.4.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "nodemon": "^2.0.16",
    "postcss": "^8.4.12",
    "postcss-preset-env": "^7.4.2",
    "prettier": "^2.7.1",
    "prisma": "^3.12.0",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "typescript": "4.6.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.tsx"
  }
}
